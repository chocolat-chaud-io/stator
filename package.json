{
  "name": "stator",
  "private": true,
  "license": "MIT",
  "scripts": {
    "affected": "nx affected",
    "affected:apps": "nx affected:apps",
    "affected:build": "nx affected:build",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected:e2e": "nx affected:e2e",
    "affected:libs": "nx affected:libs",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "build": "nx build",
    "commit": "cz",
    "dep-graph": "nx dep-graph",
    "docker:build:api": "docker build -t stator-api -f dockerfiles/api/Dockerfile .",
    "docker:build:webapp": "docker build -t stator-webapp -f dockerfiles/webapp/Dockerfile .",
    "e2e": "nx e2e",
    "format": "nx format:write",
    "format:check": "nx format:check",
    "format:prettier": "prettier --write \"./(apps|libs|tools)/**/*.(js|ts|tsx|json|yml)\"",
    "format:write": "nx format:write",
    "get-started": "npm i && npm run start --prefix tools/getting-started",
    "help": "nx help",
    "lint": "npm run affected:lint",
    "lint:file-folder-convention": "ts-node ./tools/enforce-file-folder-naming-convention.ts",
    "lint:api-imports": "ts-node ./tools/enforce-valid-imports-api.ts",
    "mongo": "run-script-os",
    "mongo:default": "sudo docker-compose -f ./apps/database/mongo/docker-compose.yml up",
    "mongo:win32": "docker-compose -f ./apps/database/mongo/docker-compose.yml up",
    "nx": "nx",
    "postgres": "run-script-os",
    "postgres:default": "sudo docker-compose -f ./apps/database/postgres/docker-compose.yml up",
    "postgres:win32": "docker-compose -f ./apps/database/postgres/docker-compose.yml up",
    "remap-api-url": "ts-node ./.do/remap-api-url.ts",
    "remap-redoc": "ts-node ./.do/remap-redoc.ts",
    "rename-project": "ts-node ./tools/rename-project.ts",
    "start": "nx serve",
    "test": "nx test",
    "typeorm": "ts-node --project tsconfig.base.json ./node_modules/typeorm/cli.js --config apps/database/orm-config.ts",
    "db:create": "ts-node --project tsconfig.base.json ./node_modules/typeorm-extension/dist/cli/index.js db:create --config apps/database/orm-config.ts",
    "db:drop": "ts-node --project tsconfig.base.json ./node_modules/typeorm-extension/dist/cli/index.js db:drop --config apps/database/orm-config.ts",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "workspace-generator": "nx workspace-generator",
    "generate-api-redux": "node node_modules/@rtk-query/codegen-openapi/lib/bin/cli.js tools/generators/open-api-config.ts && node ./tools/generators/generate-cache-key-file.js"
  },
  "husky": {
    "hooks": {
      "post-checkout": "branch-naming-check '^\\d+(-[a-z]+)+|master$'",
      "pre-commit": "branch-naming-check '^\\d+(-[a-z]+)+|master$' && npm run lint:file-folder-convention && npm run lint:api-imports",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
